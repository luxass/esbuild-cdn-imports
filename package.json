{
  "name": "esbuild-plugin-cdn-imports",
  "type": "module",
  "version": "0.1.0",
  "description": "A esbuild plugin that resolves imports directly to a CDN.",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/esbuild-cdn-imports",
  "repository": "https://github.com/luxass/esbuild-cdn-imports",
  "bugs": {
    "url": "https://github.com/luxass/esbuild-cdn-imports/issues"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm",
    "import"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.29.0",
  "scripts": {
    "build": "tsup",
    "dev": "vitest dev",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "peerDependencies": {
    "esbuild": "^0.17.14"
  },
  "dependencies": {
    "cdn-resolve": "^0.1.3",
    "ofetch": "^1.0.1",
    "path-browserify": "^1.0.1",
    "resolve.exports": "^2.0.2"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.1.0",
    "@luxass/prettier-config": "^1.2.0",
    "@types/node": "^18.15.10",
    "@types/path-browserify": "^1.0.0",
    "esbuild": "^0.17.14",
    "eslint": "^8.36.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  }
}
