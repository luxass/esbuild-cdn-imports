{
  "name": "esbuild-plugin-cdn-imports",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@8.6.6",
  "description": "A esbuild plugin that resolves imports directly to a CDN.",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/esbuild-cdn-imports",
  "repository": "https://github.com/luxass/esbuild-cdn-imports",
  "bugs": {
    "url": "https://github.com/luxass/esbuild-cdn-imports/issues"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm",
    "import"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "esbuild": "^0.17.14"
  },
  "dependencies": {
    "cdn-resolve": "^0.1.5",
    "ofetch": "^1.0.1",
    "resolve.exports": "^2.0.2"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.2.0",
    "@types/node": "^18.16.16",
    "esbuild": "^0.18.0",
    "eslint": "^8.41.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.32.0"
  }
}
