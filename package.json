{
  "name": "esbuild-plugin-cdn-imports",
  "version": "1.1.2",
  "description": "A esbuild plugin that resolves imports directly to a CDN.",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.15.3",
  "license": "MIT",
  "homepage": "https://github.com/luxass/esbuild-cdn-imports",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/esbuild-cdn-imports.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/esbuild-cdn-imports/issues"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "skypack",
    "unpkg",
    "jsdelivr",
    "esm",
    "import"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "esbuild": ">=0.19.0"
  },
  "dependencies": {
    "cdn-resolve": "^1.2.0",
    "resolve.exports": "^2.0.3"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.14.0",
    "@types/node": "^20.14.9",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
